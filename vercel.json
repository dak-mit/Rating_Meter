{
    "version": 2,
    "builds": [
        {
            "src": "new_project.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "new_project.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/var/task"
    }
} 